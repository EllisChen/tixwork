{
  "name": "nice-router-taro",
  "version": "1.0.0",
  "private": true,
  "description": "Nice-Router of Taro",
  "templateInfo": {
    "name": "redux",
    "typescript": false,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "format": "yarn prettier && yarn lint --fix",
    "prettier": "prettier --write './src/**/*'",
    "lint:fix": "eslint --fix --ext .js src && npm run lint:style",
    "lint": "eslint --ext .js src && npm run lint:style",
    "lint:style": "stylelint 'src/**/*.scss' --syntax scss",
    "precommit": "lint-staged"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "Kala <kalaliu@aliyun.com>",
  "license": "MIT",
  "dependencies": {

    "@tarojs/components": "3.0.0-beta.6",
    "@tarojs/react": "3.0.0-beta.6",
    "@tarojs/redux": "^2.2.3",
    "@tarojs/redux-h5": "^2.2.3",
    "@tarojs/runtime": "3.0.0-beta.6",
    "@tarojs/taro": "3.0.0-beta.6",
    "@tarojs/taro-h5": "2.2.3",
    "async-validator": "^3.2.4",
    "blueimp-md5": "^2.10.0",
    "classnames": "^2.2.6",
    "dva-core": "^2.0.2",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "nervjs": "^1.5.6",
    "path-to-regexp": "^3.0.0",
    "qr.js": "^0.0.0",
    "qs": "^6.7.0",
    "react": "^16.10.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.0.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "taro-ui": "^3.0.0-alpha.2",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/runtime": "^7.7.7",
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@tarojs/mini-runner": "3.0.0-beta.6",
    "@tarojs/webpack-runner": "3.0.0-beta.6",
    "@types/react": "^16.0.0",
    "@types/webpack-env": "^1.13.6",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.11",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.0.0-beta.6",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-taro": "^3.0.0-beta.6",
    "lint-staged": "^7.2.2",
    "prettier": "^1.17.0",
    "stylelint": "9.3.0",
    "babel-preset-taro": "^3.0.0-alpha.8",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.0.0",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,scss}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.scss": "stylelint --syntax scss"
  }
}
